@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix db: <http://mappings.dbpedia.org/server/ontology/classes/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix gn: <http://geonames.org/> .
@prefix wd: <http://worldbank.org/> .
@base <http://geonames.org/> .

<ContinentCodesTriples>
    # Define the data source
    rml:logicalSource [
        rml:source "/home/alescop/Documents/University/BMKG/knowledge_graphs/data/continent-codes.csv";
        rml:referenceFormulation ql:CSV
    ];

    # Define the subject of the generated statements
    rr:subjectMap [
        rr:template "http://dbpedia.org/page/{Name}";
#       rr:class db:Continent
    ].

<GeoNamesTriples>
    # Define the data source (file location)
    rml:logicalSource [
        rml:source "/home/alescop/Documents/University/BMKG/knowledge_graphs/data/countryInfo.csv";
        rml:referenceFormulation ql:CSV
    ];

    # Define the subject of the generated statements
    rr:subjectMap [
        rr:template "http://geonames.org/countries/{ISO}/{Country}.html";
        rr:class db:Country
    ];

    # Map the country name to rdfs:label
    rr:predicateObjectMap [ 
        rr:predicate rdfs:label;
        rr:objectMap [ 
            rml:reference "Country"
        ]
    ];
    
    # Define the object of the population property as an integer
    rr:predicateObjectMap [ 
        rr:predicate db:Population;
        rr:objectMap [ 
            rml:reference "Population"; 
            rr:datatype xsd:integer 
        ]
    ];

    # Generate a URI for the continent object
    rr:predicateObjectMap [ 
        rr:predicate db:Continent;
        rr:objectMap [
            rr:parentTriplesMap <ContinentCodesTriples>;
            rr:joinCondition [
                rr:child "Continent";
                rr:parent "Code";
            ];
        ];
    ].
        
